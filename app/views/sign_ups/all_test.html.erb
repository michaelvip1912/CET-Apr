<% provide(:title, '历次报名信息') %>
<h1>历次报名信息</h1>

<div class="users">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>报名描述</th>
      </tr>
    </thead>

    <tbody>
      <% @all_tests.each do |all_test| %>
        <tr>
         <td><%= link_to all_test.description, :action =>'every_test', :id => all_test  %></td>
        </tr>
      <% end %>
   </tbody>
 </table>
</div>