<% provide(:title, "登录") %>
<h1>考试报名系统</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :encode, "帐号" %>
      <%= f.text_field :encode %>

      <%= f.label :password, "密码"%>
      <%= f.password_field :password %>

      <%= f.submit "登录", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>